  <ul>
    <li><%= category.name %>
    	<%= link_to 'Show', category %>
    	<%= link_to 'Edit', edit_category_path(category) %>
    	<%= link_to 'Destroy', category, confirm: 'Are you sure?', method: :delete %>
    </li>
    
    <% Category.where(:category_id => category.id).each do |child_category| %>
		<%= render :partial => "category_new", :locals => {:category => child_category} %>
  	<% end %>
  </ul>