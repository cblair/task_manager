<h1>Listing work_times</h1>

<table>
  <tr>
    <th>Start time</th>
    <th>Endtime</th>
    <th>Task</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @work_times.each do |work_time| %>
  <tr>
    <td><%= work_time.start_time %></td>
    <td><%= work_time.end_time %></td>
    <td><%= work_time.task_id %></td>
    <td><%= link_to 'Show', work_time %></td>
    <td><%= link_to 'Edit', edit_work_time_path(work_time) %></td>
    <td><%= link_to 'Destroy', work_time, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Work time', new_work_time_path %>
